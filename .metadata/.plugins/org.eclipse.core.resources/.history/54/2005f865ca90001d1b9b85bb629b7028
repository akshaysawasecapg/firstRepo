package comparatorEx;

import java.util.Comparator;

public class SortByaName implements Comparator<Revenue> {

	
	public int compare(Revenue r1, Revenue r2){
		if(r1.revenueName == r2.revenueName){
			return 0;
		}else{
			if(r1.revenueName < r2.revenueName){
				return -1;
			}else{
				return 1;
			}
		}
	}
}
